<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#CFD8DC"
    tools:context=".GameActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="409dp"
        android:layout_height="729dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/playAgainBtn"
            android:layout_width="150dp"
            android:layout_height="50dp"
            android:layout_marginBottom="88dp"
            android:background="@drawable/playagain_button"
            android:onClick="playAgainButton"
            android:text="@string/playAgainButton"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.521"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:text="Ultimate TicTacToe Game"
            android:textSize="34sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.687"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.example.myapplication.TicTacToeBoard
            android:id="@+id/ticTacToeBoard2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.475"
            custom:LineColor="#00FF00"
            custom:OColor="#1A237E"
            custom:XColor="#1A237E"
            custom:boardColor="#3F51B5" />

        <ImageView
            android:id="@+id/image1"
            android:layout_width="128dp"
            android:layout_height="131dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="152dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:srcCompat="@drawable/x" />

        <ImageView
            android:id="@+id/image2"
            android:layout_width="125dp"
            android:layout_height="132dp"
            android:layout_marginStart="4dp"
            android:layout_marginTop="152dp"
            android:layout_marginEnd="144dp"
            app:layout_constraintEnd_toEndOf="@+id/ticTacToeBoard2"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/image1"
            app:layout_constraintTop_toTopOf="parent"
            custom:srcCompat="@drawable/empty" />

        <ImageView
            android:id="@+id/image3"
            android:layout_width="127dp"
            android:layout_height="132dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="152dp"
            app:layout_constraintStart_toEndOf="@+id/image2"
            app:layout_constraintTop_toTopOf="parent"
            custom:srcCompat="@drawable/empty" />

        <ImageView
            android:id="@+id/image4"
            android:layout_width="135dp"
            android:layout_height="140dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/image1"
            custom:srcCompat="@drawable/empty" />

        <ImageView
            android:id="@+id/image5"
            android:layout_width="131dp"
            android:layout_height="135dp"
            android:layout_marginStart="4dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="139dp"
            app:layout_constraintEnd_toEndOf="@+id/ticTacToeBoard2"
            app:layout_constraintStart_toEndOf="@+id/image4"
            app:layout_constraintTop_toBottomOf="@+id/image2"
            custom:srcCompat="@drawable/empty" />

        <ImageView
            android:id="@+id/image6"
            android:layout_width="140dp"
            android:layout_height="137dp"
            android:layout_marginStart="4dp"
            android:layout_marginTop="4dp"
            app:layout_constraintStart_toEndOf="@+id/image5"
            app:layout_constraintTop_toBottomOf="@+id/image3"
            custom:srcCompat="@drawable/empty" />

        <ImageView
            android:id="@+id/image7"
            android:layout_width="130dp"
            android:layout_height="131dp"
            android:layout_marginTop="4dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/image4"
            custom:srcCompat="@drawable/empty" />

        <ImageView
            android:id="@+id/image8"
            android:layout_width="134dp"
            android:layout_height="133dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="6dp"
            app:layout_constraintStart_toEndOf="@+id/image7"
            app:layout_constraintTop_toBottomOf="@+id/image5"
            custom:srcCompat="@drawable/empty" />

        <ImageView
            android:id="@+id/image9"
            android:layout_width="140dp"
            android:layout_height="139dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/image8"
            app:layout_constraintTop_toBottomOf="@+id/image6"
            custom:srcCompat="@drawable/empty" />

        <ImageView
            android:id="@+id/image10"
            android:layout_width="307dp"
            android:layout_height="44dp"
            android:layout_marginBottom="36dp"
            app:layout_constraintBottom_toTopOf="@+id/ticTacToeBoard2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            custom:srcCompat="@drawable/empty" />

        <ImageView
            android:id="@+id/imageWin1"
            android:layout_width="257dp"
            android:layout_height="275dp"
            android:rotation="35"
            android:scaleX="3"
            android:scaleY="1"
            app:layout_constraintBottom_toBottomOf="@+id/ticTacToeBoard2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/ticTacToeBoard2"
            custom:srcCompat="@drawable/empty" />

        <ImageView
            android:id="@+id/imageWin2"
            android:layout_width="265dp"
            android:layout_height="263dp"
            android:rotation="-35"
            android:scaleX="3"
            android:scaleY="1"
            app:layout_constraintBottom_toBottomOf="@+id/ticTacToeBoard2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/ticTacToeBoard2"
            custom:srcCompat="@drawable/empty" />

        <ImageView
            android:id="@+id/imageWin3"
            android:layout_width="257dp"
            android:layout_height="275dp"
            android:scaleX="3"
            android:scaleY="1"
            app:layout_constraintBottom_toBottomOf="@+id/image2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.7"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/ticTacToeBoard2"
            custom:srcCompat="@drawable/empty" />

        <ImageView
            android:id="@+id/imageWin4"
            android:layout_width="257dp"
            android:layout_height="275dp"
            android:scaleX="3"
            android:scaleY="1"
            app:layout_constraintBottom_toTopOf="@+id/image8"
            app:layout_constraintEnd_toEndOf="@+id/ticTacToeBoard2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/image2"
            custom:srcCompat="@drawable/empty" />

        <ImageView
            android:id="@+id/imageWin5"
            android:layout_width="257dp"
            android:layout_height="275dp"
            android:scaleX="3"
            app:layout_constraintBottom_toBottomOf="@+id/ticTacToeBoard2"
            app:layout_constraintEnd_toEndOf="@+id/image9"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/image5"
            custom:srcCompat="@drawable/empty" />

        <ImageView
            android:id="@+id/imageWin6"
            android:layout_width="257dp"
            android:layout_height="275dp"
            android:scaleX="1"
            android:scaleY="2"
            app:layout_constraintBottom_toBottomOf="@+id/image7"
            app:layout_constraintEnd_toEndOf="@+id/image4"
            app:layout_constraintHorizontal_bias="0.065"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/image1"
            app:layout_constraintVertical_bias="0.462"
            custom:srcCompat="@drawable/empty" />

        <ImageView
            android:id="@+id/imageWin7"
            android:layout_width="257dp"
            android:layout_height="275dp"
            android:scaleX="1"
            android:scaleY="2"
            app:layout_constraintBottom_toBottomOf="@+id/ticTacToeBoard2"
            app:layout_constraintEnd_toEndOf="@+id/image5"
            app:layout_constraintStart_toEndOf="@+id/image4"
            app:layout_constraintTop_toTopOf="@+id/ticTacToeBoard2"
            custom:srcCompat="@drawable/empty" />

        <ImageView
            android:id="@+id/imageWin8"
            android:layout_width="257dp"
            android:layout_height="275dp"
            android:scaleX="1"
            android:scaleY="2"
            app:layout_constraintBottom_toBottomOf="@+id/image9"
            app:layout_constraintEnd_toEndOf="@+id/ticTacToeBoard2"
            app:layout_constraintStart_toEndOf="@+id/image5"
            app:layout_constraintTop_toTopOf="@+id/image3"
            custom:srcCompat="@drawable/empty" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>